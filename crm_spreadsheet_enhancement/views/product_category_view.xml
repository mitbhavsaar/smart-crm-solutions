<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="view_product_category_form_inherit_raisin" model="ir.ui.view">
        <field name="name">product.category.form.raisin</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">

            <xpath expr="//sheet" position="inside">
                <group string="Calculation Template">
                    <!-- File Upload Section -->
                    <field name="template_file" filename="template_filename"/>
                    <field name="template_filename" invisible="1"/>
                    
                </group>
            </xpath>
        </field>
    </record>
</odoo>
